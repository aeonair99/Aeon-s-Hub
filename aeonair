local Misc = Window:NewTab("Misc")
local MiscSection = Misc:NewSection("Misc")


MiscSection:NewButton("Cloak TP Tool", "Better version of apparate.", function()
    local player = game.Players.LocalPlayer
    local mouse = player:GetMouse()
    local Animate 
    local Humanoid = player.Character:FindFirstChild('Humanoid')
    
    
    mouse = game.Players.LocalPlayer:GetMouse()
    tool = Instance.new("Tool")
    tool.RequiresHandle = false
    tool.Name = "tptool"
    tool.Activated:connect(function()
     local Animation = Instance.new("Animation", player.Character)
      Animation.AnimationId = "rbxassetid://5"
      Animate = Humanoid:LoadAnimation(Animation)
      Animate:Play(.1, 1, 1)
    
    Animate:Stop()
    game.ReplicatedStorage.Events.WandEvent:FireServer("flight", true)
    
    game.ReplicatedStorage.Events.WandEvent:FireServer("Cloak", true)
    
    
    
    
    
     local pos = mouse.Hit.p
    pos = CFrame.new(pos.X,pos.Y + 3.5,pos.Z)
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = pos
    
    game.ReplicatedStorage.Events.WandEvent:FireServer("Cloak", false)
    
    game.ReplicatedStorage.Events.WandEvent:FireServer("flight", false)
    
    
    Animate:Stop()
    end)
    tool.Parent = game.Players.LocalPlayer.Backpack
    
end)

MiscSection:NewButton("Presentus Tool", "When ur birthday is on christmas.", function()
    mouse = game.Players.LocalPlayer:GetMouse()
tool = Instance.new("Tool")
tool.RequiresHandle = false
tool.Name = "Present"
tool.Activated:connect(function()
 game.ReplicatedStorage.Events.WandEvent:FireServer("presentus")
end)
tool.Parent = game.Players.LocalPlayer.Backpack
end)

MiscSection:NewButton("Cloak Spam", "G to toggle", function()
    local solar = false
    local cylane = "g"
    
    local m = game:GetService"Players".LocalPlayer:GetMouse()
    m.KeyDown:connect(function(k)
       if k == cylane then
           solar = not solar
       end
    end)
    
    spawn(function()
       while wait() do
           if solar then
    game.ReplicatedStorage.Events.WandEvent:FireServer("Cloak", true)
    
    game.ReplicatedStorage.Events.WandEvent:FireServer("Cloak", false)
    
    game.ReplicatedStorage.Events.WandEvent:FireServer("flight", true)
    
    game.ReplicatedStorage.Events.WandEvent:FireServer("flight", false)
           end
       end
    end)

end)

