MiscSection:NewButton("Avada Rain", "I don't got any other ideas for the name so ig this is it.", function()
    local q   = "avada kedavra"
    local Key = "z" -- Key that toggles script
    local plr       = game.Players.LocalPlayer
    local char       =  plr.Character
    local mouse       =  plr:GetMouse()
    local KeyDown = false
    _G.toggle  = false
 
 function shoot()
        local v = (mouse.hit.p - game.Workspace.CurrentCamera.CoordinateFrame.p).unit
        --original speed is 625
 local w = v *  math.random(1000, 1000)
        local x = 1
        local y = game.Workspace.CurrentCamera.CoordinateFrame.p + v + Vector3.new(math.random(-50,50), math.random(1,1), math.random(-50,50)) + -v * math.random(10,10)
                plr.PlayerGui.Client.fwp:Fire(y, w, true, q)
                game.ReplicatedStorage.Events.ProjEvent:FireServer(y, w, q, x)
    end
 mouse.KeyDown:connect(function(key)
    if key == Key then
        if _G.toggle == true then
            _G.toggle = false
        else if _G.toggle == false then
            _G.toggle     = true
        end
        end
    end
 end)
 
 
 
 
 mouse.Button1Down:connect(function()
    KeyDown       = true
    while KeyDown == true and _G.toggle do
 
 shoot()
 
 wait(0.05)
    end
 end)
 
 
 mouse.Button1Up:connect(function()
    KeyDown = false
 end)
end)

MiscSection:NewButton("Avada Beam", "Big Black Monkey Balls", function()
    local q   = "avada kedavra"
    local Key = "z" -- Key that toggles script
    local plr       = game.Players.LocalPlayer
    local char       =  plr.Character
    local mouse       =  plr:GetMouse()
    local KeyDown = false
    _G.toggle  = false
 
 function shoot()
        local v = (mouse.hit.p - game.Workspace.CurrentCamera.CoordinateFrame.p).unit
        --original speed is 625
 local w = v *  math.random(100, 1000)
        local x = 99999999999999
        local y = game.Workspace.CurrentCamera.CoordinateFrame.p + v
                plr.PlayerGui.Client.fwp:Fire(y, w, true, q)
                game.ReplicatedStorage.Events.ProjEvent:FireServer(y, w, q, x)
    end
 mouse.KeyDown:connect(function(key)
    if key == Key then
        if _G.toggle == true then
            _G.toggle = false
        else if _G.toggle == false then
            _G.toggle     = true
        end
        end
    end
 end)
 
 
 
 
 mouse.Button1Down:connect(function()
    KeyDown       = true
    while KeyDown == true and _G.toggle do
 
 
 
  shoot()
 
 wait(0.00000001)
  end
 end)
 
 
 mouse.Button1Up:connect(function()
    KeyDown = false
 end)
end)

