MiscSection:NewButton("Avada Storm", "Z to toggle all avada scripts.", function()
    local q   = "avada kedavra"
    local Key = "z" -- Key that toggles script
    local plr       = game.Players.LocalPlayer
    local char       =  plr.Character
    local mouse       =  plr:GetMouse()
    local KeyDown = false
    _G.toggle  = false
    
 function shoot()
        local v = (mouse.Hit.p).unit * Vector3.new(math.random(-30, 30),-50,math.random(-30, 30))
        local w = v * 100
        local x = math.random(100, 1000)
        local y = mouse.Hit.p + -v + Vector3.new(math.random(-20, 20), math.random(200, 2000), math.random(-20, 20)) + -v * math.random(0, 0)
                plr.PlayerGui.Client.fwp:Fire(y, w, true, q)
                game.ReplicatedStorage.Events.ProjEvent:FireServer(y, w, q, x)
    end
 mouse.KeyDown:connect(function(key)
    if key == Key then
        if _G.toggle == true then
            _G.toggle = false
        else if _G.toggle == false then
            _G.toggle     = true
        end
        end
    end
 end)
 
